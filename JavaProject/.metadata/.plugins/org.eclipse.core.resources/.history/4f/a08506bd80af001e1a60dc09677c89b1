package day0110;

class Ipgo {
	private String sangName;
	private int Price;
	
	//set메서드
	public void setName(String setName) {
		sangName = setName;
	}
	
	public void setPrice(int sprice) {
		Price = sprice;
	}
	//get메서드
	public String getName() {
		return sangName;
		
	}
	public int getPrice() {
		return Price;
		
	}
	
}

public class SangpumIpgo_13 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Ipgo ipgo1 = new Ipgo();
		ipgo1.sangName
	}

}
