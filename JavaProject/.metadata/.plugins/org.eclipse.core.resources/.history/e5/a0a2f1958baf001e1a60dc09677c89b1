package day0110;

class Mart {
	private String sangpum;
	int su;
	int dan;
	static String SHOPNAME = "롯데마트";

	public Mart() {

	}
	
	public Mart(String sang,int su,int dan) {
		sangpum = sang;
		su = su;
		dan = dan;
		
	}
	
	public void name(String a) {
		sangpum = a;
	}
	public void su(int b) {
		su = b;
	}
	public void dan(int c) {
		dan = c;
	}
}

public class QuizClass_16 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Mart m1 = new Mart("초코파이", 10, 4500);
		Mart m2 = new Mart("엄마손파이", 5, 2500);
		
		String name = m1.name();
		int su = m1.su();
		int dan = m1.dan();
		

		
		
	}

}
