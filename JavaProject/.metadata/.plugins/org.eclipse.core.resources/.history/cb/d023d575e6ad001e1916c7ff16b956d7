package day0108;

public class QuizFor_12 {

	public static void main(String[] args) {

		System.out.println("1. 1 2 4 5 7 8 10 (continue활용할것!!)");
		int a = 1;
		for (; a <= 10; a++) {
			if (a % 3 == 0) {
				continue;
			} 
			System.out.print(a + "  ");
			
		}
		
		System.out.println("홀수값:1 홀수값:5 홀수값:5 홀수값:7 홀수값:9");
		
		int i = 1;
		for(; i<=10; i++) {
			if (i % 2 == 0) {
				continue;
			}
			System.out.print("홀수값 : " + i + "  ");
		}
		
	}

}
